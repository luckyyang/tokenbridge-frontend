{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "indent": [
      true,
      "spaces",
      2
    ],
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "no-object-literal-type-assertion": false,
    "no-console": [
      false
    ],
    "no-var-requires": false,
    "object-literal-sort-keys": false
  }
}
